<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans default-autowire="byName">
	
    <bean id="seedClient" class="com.taobao.gaia.AppServer">
        <property name="ip" value="10.232.4.4"/>
       	<property name="pid" value="-1"/>
       	<property name="confname" value="/home/admin/workspace/mingyan.zc/tfstest_new/test_tfs.conf" />
       	<property name="dir" value="/home/admin/workspace/mingyan.zc/tfstest_new" />
       	<property name="logs" value="/home/admin/workspace/mingyan.zc/tfstest_new/tfsSeed." />
        <property name="isBackground" value="true" />
		<property name="startcmd">
		<value>/home/admin/workspace/mingyan.zc/tfstest_new/tfsControlPress -f /home/admin/workspace/mingyan.zc/tfstest_new/test_tfs.conf -i tfsSeed -l /home/admin/workspace/mingyan.zc/tfstest_new/tfsSeed.log</value>
		</property>
		<property name="stopcmd" value="killall tfsControlPress" />
		<property name="cleancmd">
			<list>
				<value>rm -rf *.log *.txt</value>
			</list>
		</property>
    </bean>
    
