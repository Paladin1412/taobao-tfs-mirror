<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<!-- 具有resource contro（RC)功能的客户端bean初始化。since (V2.1.4).-->
<beans>
  <bean id="tfsManager" class="com.taobao.common.tfs.DefaultTfsManager" init-method="init" >
    <!-- rc server的地址 (必须配置）-->
    <property name="rcAddr">
      <!--  value>10.232.36.206:6261</value>-->
      <!--  <value>10.232.4.8:6269</value>>-->
       <value>10.232.4.5:6269</value>
     <!--  <value>10.232.36.203:6100</value>  -->
    </property>
    <!-- 应用注册的App Key (必须配置）-->
    <property name="appKey">
    	 <value>testKeyYS</value>
    	<!--<value>tfscom</value>--> 
      <!--value>foobarRcAA</value-->
    </property>
    <!-- 应用的ip地址，可能有代理的存在，不一定是localIp (必须配置）-->
    <property name="appIp">
      <value>10.232.4.8</value>
    </property>
    <!-- 使用是否使用自定义目录文件名功能，默认false.(可选配置)-->
    <property name="useNameMeta">
      <value>true</value>
    </property>
    <!-- 超时时间（可选配置）-->
    <property name="timeout">
      <value>5000</value>
    </property>
    <!-- block缓存最大有效的时间(ms) （可选配置）!-->
    <property name="maxCacheTime">
      <value>600000</value>
    </property>
    <!-- block缓存的最大数量 （可选配置）-->
    <property name="maxCacheItemCount">
      <value>10000</value>
    </property>
  </bean>
</beans>
